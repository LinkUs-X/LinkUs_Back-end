<p id="notice"><%= notice %></p>

<h1></h1>

<table>
  <thead>
    <tr>
      <th>Link Id</th>
      <th>User Id</th>
      <th>User login</th>
      <th>Card Id</th>
      <th>Contact Login</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Created at</th>
      <th>Updated at</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% if (defined? @link1) == "instance-variable" && (defined? @link2) == "instance-variable"
        @links = [@link1, @link2]
        @links.each do |link| %>
        <tr>
         <td><%= link.id %></td>  
         <td><%= link.user_id %></td>
         <td><%= User.find(link.user_id).login %></td>
         <td><%= link.card_id %></td>
         <td><%= User.find(Card.find(link.card_id).user_id).login %></td>
         <td><%= link.lat %></td>
         <td><%= link.lng %></td>
         <td><%= link.created_at %></td>
         <td><%= link.updated_at %></td>
        </tr>
        <% end %>
    <% end %>
  </tbody>
</table>

<br>